our_routes = {"LAX", "JFK", "CDG", "DXB"}
competitor_routes = {"JFK", "CDG", "LHR", "BKK"}

#destinations both airlines fly 
joint_routes= our_routes.intersection(competitor_routes)
print (joint_routes) 

#destinations unque to our_routes

unique_routes= our_routes.difference(competitor_routes)
print (unique_routes)

#Destinations neither airline shares 

diff_routes= our_routes.symmetric_difference (competitor_routes)
print (diff_routes)

customer_ids = ["C001", "C002", "C003", "C002", "C001", "C004"]
unique_ids= set(customer_ids)
print (unique_ids)